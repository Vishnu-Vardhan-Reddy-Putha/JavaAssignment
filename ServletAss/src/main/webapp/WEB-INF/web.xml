<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<display-name>RegisterServlet</display-name>
		<description></description>
		<servlet-class>com.servlet.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GradeServlet</servlet-name>
		<display-name>GradeServlet</display-name>
		<description></description>
		<servlet-class>servletrequestdispatcher.GradeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InputServlet</servlet-name>
		<display-name>InputServlet</display-name>
		<description></description>
		<servlet-class>servletrequestdispatcher.InputServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GradeServlet</servlet-name>
		<url-pattern>/grade</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InputServlet</servlet-name>
		<url-pattern>/input</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>studentForm.jsp</welcome-file>
	</welcome-file-list>

</web-app>
